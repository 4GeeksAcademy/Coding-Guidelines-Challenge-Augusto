{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
  },
  "rules": {
    "strict": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "semi": ["error", "always"],
    "allowImportExportEverywhere": false,
    "comma-dangle": [1, {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "ignore",
    }],
  },
  "extends": ["eslint:recommended"]
}
